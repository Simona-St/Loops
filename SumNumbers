using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace sum
{
    class Program
    {
        static void Main(string[] args)
        {

            Console.Write("Моля въведете брой числа n= ");
            var n = int.Parse(Console.ReadLine());
            Console.WriteLine("Моля въведете {0} числа, всяко на отделен ред:", n);
            var sum = 0;

            for (int i = 0; i < n; i++)
            {
                Console.Write("num {0} = ", i);
                var num = int.Parse(Console.ReadLine());
                sum = sum + num;
            }

            Console.WriteLine("Сумата е " + sum);
        }
    }
}
